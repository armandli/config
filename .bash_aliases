#prevent-error-during-typing kind of aliases
alias cd..="cd .."
alias cd-="cd -"
alias ll="ls -al"

#easy ps
alias psu="ps -ef | grep $WHOAMI"

#grep with colour
alias grep='grep --color=tty -d skip'

#c++11 by default
alias g++11='g++ --std=gnu++11'
alias clang++11='clang++ --std=gnu++11'

